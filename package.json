{
  "name": "eyesbound",
  "private": true,
  "description": "Website for Berlin based Photographer",
  "version": "3.0.0",
  "license": "0BSD",
  "author": {
    "name": "Jannis Hell"
  },
  "engines": {
    "node": ">=22.4.0"
  },
  "scripts": {
    "build": "gatsby build",
    "clean": "gatsby clean",
    "coverage": "JEST_JUNIT_CLASSNAME=\"{filepath}\" jest --reporters=jest-junit --ci --coverage",
    "develop": "gatsby clean && gatsby develop -S",
    "lint": "xo --config eslint.config.mjs",
    "lint:fix": "npm run lint -- --fix",
    "lint:watch": "npx nodemon -x npm run lint:fix --ext js,jsx,json",
    "serve": "gatsby serve",
    "start": "npm run develop",
    "start:lan": "npm run develop -- -o -H 0.0.0.0 -p 8000",
    "test": "jest",
    "test:update": "npm test -- -u",
    "test:watch": "npm test -- --watch"
  },
  "dependencies": {
    "@emotion/react": "^11.14.0",
    "@emotion/styled": "^11.14.1",
    "@fontsource/montserrat": "^5.2.8",
    "@react-google-maps/api": "^2.20.8",
    "classnames": "^2.5.1",
    "gatsby": "^5.16.1",
    "gatsby-plugin-csp": "^1.1.4",
    "gatsby-plugin-emotion": "^8.16.0",
    "gatsby-plugin-image": "^3.16.0",
    "gatsby-plugin-manifest": "^5.16.0",
    "gatsby-plugin-netlify": "^5.1.1",
    "gatsby-plugin-offline": "^6.16.0",
    "gatsby-plugin-react-helmet": "^6.16.0",
    "gatsby-plugin-robots-txt": "^1.8.0",
    "gatsby-plugin-sitemap": "^6.16.0",
    "gatsby-plugin-web-font-loader": "^1.0.4",
    "gatsby-source-prismic": "^6.0.3",
    "i18next": "^25.8.10",
    "prop-types": "^15.8.1",
    "react": "^19.2.4",
    "react-dom": "^19.2.4",
    "react-helmet": "^6.1.0",
    "react-i18next": "^16.5.4",
    "react-slideshow-image": "^4.3.2"
  },
  "devDependencies": {
    "@emotion/babel-plugin": "^11.13.5",
    "@emotion/jest": "^11.14.2",
    "@testing-library/dom": "^10.4.1",
    "@testing-library/jest-dom": "^6.9.1",
    "@testing-library/react": "^16.3.2",
    "ajv": "^8.18.0",
    "babel-jest": "^30.2.0",
    "dotenv": "^17.3.1",
    "eslint": "^10.0.0",
    "eslint-config-xo-react": "^0.29.0",
    "eslint-plugin-jest": "^29.15.0",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^30.2.0",
    "jest-environment-jsdom": "^30.2.0",
    "jest-junit": "^16.0.0",
    "typescript": "^5.9.3",
    "xo": "^1.2.3"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/Primajin/eyesbound"
  }
}
