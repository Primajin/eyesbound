    "gatsby-source-prismic": "^3.3.3",
    "prop-types": "^15.7.2",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "react-helmet": "^6.1.0",
    "react-slideshow-image": "^3.4.3"
  },
  "devDependencies": {
    "dotenv": "^8.2.0",
    "eslint": "^7.19.0",
    "eslint-config-xo": "^0.34.0",
    "eslint-config-xo-react": "^0.23.0",
    "eslint-plugin-ava": "^11.0.0",
