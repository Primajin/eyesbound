{
  "name": "eyesbound",
  "private": true,
  "description": "Website for Berlin based Photographer",
  "version": "3.0.0",
  "license": "0BSD",
  "author": {
    "name": "Jannis Hell"
  },
  "engines": {
    "node": ">=16"
  },
  "scripts": {
    "prebuild": "xo",
    "build": "gatsby build",
    "clean": "gatsby clean",
    "develop": "gatsby develop",
    "format": "npm run xo",
    "serve": "gatsby serve",
    "start": "npm run develop",
    "test": "xo && echo \"Write tests! -> https://gatsby.dev/unit-testing\" && exit 1",
    "xo": "xo --fix",
    "xo:watch": "npx nodemon -x npm run xo --ext js,jsx,json"
  },
  "dependencies": {
    "@emotion/react": "^11.4.1",
    "@emotion/styled": "^11.3.0",
    "@react-google-maps/api": "^2.2.0",
    "classnames": "^2.3.1",
    "gatsby": "^3.13.0",
    "gatsby-plugin-csp": "^1.1.3",
    "gatsby-plugin-emotion": "^6.13.0",
    "gatsby-plugin-gatsby-cloud": "^3.1.0",
    "gatsby-plugin-image": "^1.13.0",
    "gatsby-plugin-manifest": "^3.13.0",
    "gatsby-plugin-offline": "^4.13.0",
    "gatsby-plugin-react-helmet": "^4.13.0",
    "gatsby-plugin-sitemap": "^4.9.0",
    "gatsby-source-prismic": "^4.1.2",
    "prop-types": "^15.7.2",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-helmet": "^6.1.0",
    "react-slideshow-image": "^3.5.0"
  },
  "devDependencies": {
    "dotenv": "^10.0.0",
    "eslint-config-xo": "^0.38.0",
    "eslint-config-xo-react": "^0.25.0",
    "xo": "^0.44.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/Primajin/eyesbound"
  },
  "xo": {
    "extends": [
      "xo",
      "xo-react"
    ],
    "rules": {
      "node/prefer-global/process": "off"
    }
  },
  "eslintConfig": {
    "env": {
      "browser": false,
      "node": true,
      "es6": true
    },
    "extends": [
      "xo-react",
      "./node_modules/xo/config/plugins.js"
    ]
  },
  "eslintIgnore": [
    "gatsby-config.js",
    "gatsby-node.js"
  ]
}